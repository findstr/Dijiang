---
brdf_texture:
        type: texture2d
        file: asset/lut/ibl_brdf_lut.png
skybox:
    specular:
        type: cubemap
        x+: asset/textures/skybox/skybox_specular_X+.hdr
        x-: asset/textures/skybox/skybox_specular_X-.hdr
        y+: asset/textures/skybox/skybox_specular_Z+.hdr
        y-: asset/textures/skybox/skybox_specular_Z-.hdr
        z+: asset/textures/skybox/skybox_specular_Y+.hdr
        z-: asset/textures/skybox/skybox_specular_Y-.hdr
    irradiance:
        type: cubemap
        x+: asset/textures/skybox/skybox_irradiance_X+.hdr
        x-: asset/textures/skybox/skybox_irradiance_X-.hdr
        y+: asset/textures/skybox/skybox_irradiance_Z+.hdr
        y-: asset/textures/skybox/skybox_irradiance_Z-.hdr
        z+: asset/textures/skybox/skybox_irradiance_Y+.hdr
        z-: asset/textures/skybox/skybox_irradiance_Y-.hdr