---
root:
    - gameobject:
        name: Camera
        components:
                - transform:
                    position:
                        x: +0
                        y: +0
                        z: -10
                    rotation:
                        x: -10
                        y: +0
                        z: +0
                    scale:
                        x: +1
                        y: +1
                        z: +1
                - camera:
                        fov: +30
                        aspect: +1.8
                        clip_near_plane: +1.0
                        clip_far_plane: +100.0
                - component.main:
                        speed: +1.0

    - gameobject:
        name: Player
        components:
                - transform:
                    position:
                        x: +0
                        y: -0.5
                        z: +0
                    rotation:
                        x: -90
                        y: +0
                        z: +0
                    scale:
                        x: +1
                        y: +1
                        z: +1
                - meshfilter:
                    - submesh:
                        mesh: asset/models/viking_room.obj
                - meshrender:
                        material: asset/material/test.mat
